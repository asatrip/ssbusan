<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% articles.forEach(article => { %>
  <url>
    <loc
      ><%= locals.setting.siteDomain %>/<%= article.boardSlugRaw %>/<%=
      article.slug %></loc
    >
    <lastmod><%= article.datetime %></lastmod>
  </url>
  <% }) %>
</urlset>

<!DOCTYPE html>
<html lang="ko">
  <head>
    <%- include('../head') %>
    <meta
      property="og:url"
      content="<%= setting.siteDomain %>/mypage/article"
    />
    <link rel="canonical" href="<%= setting.siteDomain %>/mypage/article" />
    <link rel="stylesheet" href="/stylesheets/user/user.css" />
  </head>
  <body>
    <%- include('../commons') %> <%- include('../header') %>
    <main>
      <%- include('../aside', { mobile: false }) %>
      <article id="myArticle">
        <%- include('../banner', { position: 'indexTop' }) %> 
        <%- include('../banner', { position: 'articleTop' }) %> 
        <%- include('./header') %> <%- include('../title', { title:
        lang.user_myArticles }) %>
        <section id="item">
          <div class="items">
            <% articles.forEach(article => { %>
            <div class="item">
              <div class="main">
                <a href="/<%= article.boardSlug %>/<%= article.slug %>"
                  ><%= article.title %></a
                >
              </div>
              <div class="info">
                <div class="datetime"><%= article.datetime %></div>
              </div>
            </div>
            <% }) %>
          </div>
        </section>
        <%- include('../pagination') %>
      </article>
      <%- include('../aside', { mobile: true }) %>
    </main>
    <%- include('../footer') %>
  </body>
</html>
